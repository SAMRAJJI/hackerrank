    "class Solution:\n",
    "\tdef pushZerosToEnd(self,arr):\n",
    "\t    count=0\n",
    "\t    for i in range(len(arr)):\n",
    "\t       if arr[i] !=0:\n",
    "\t           arr[i],arr[count]=arr[count], arr[i]\n",
    "\t           count+=1"
   ]
  }
