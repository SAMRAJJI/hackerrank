    "def getMoneySpent(keyboards, drives, b):\n",
    "    val= -1\n",
    "    for i in keyboards:\n",
    "        for j in drives:\n",
    "            total= i+j\n",
    "            if total <=b and val< total:\n",
    "                val= total\n",
    "    return val"
   ]
